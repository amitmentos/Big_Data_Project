{
  "data_asset_type": null,
  "expectation_suite_name": "bronze_raw_user_events",
  "expectations": [
    {
      "expectation_type": "expect_table_columns_to_match_ordered_list",
      "kwargs": {
        "column_list": [
          "event_id",
          "customer_id", 
          "event_type",
          "product_id",
          "session_id",
          "event_time",
          "platform",
          "device_type",
          "location",
          "page_url",
          "referrer_url",
          "search_query",
          "last_updated_day"
        ]
      },
      "meta": {
        "description": "Ensure all required columns are present in correct order"
      }
    },
    {
      "expectation_type": "expect_table_row_count_to_be_between",
      "kwargs": {
        "min_value": 1,
        "max_value": 1000000
      },
      "meta": {
        "description": "Table should have between 1 and 1M rows"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "event_id"
      },
      "meta": {
        "description": "Event ID should never be null"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "event_type"
      },
      "meta": {
        "description": "Event type should never be null"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "event_time"
      },
      "meta": {
        "description": "Event time should never be null"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_unique",
      "kwargs": {
        "column": "event_id"
      },
      "meta": {
        "description": "Event IDs should be unique"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "event_type",
        "value_set": [
          "page_view",
          "product_view", 
          "add_to_cart",
          "remove_from_cart",
          "add_to_wishlist",
          "search",
          "filter_apply",
          "sort_apply",
          "checkout_start",
          "purchase_complete",
          "user_login",
          "user_logout"
        ]
      },
      "meta": {
        "description": "Event type should be one of the predefined values"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "platform",
        "value_set": ["web", "mobile", "tablet", "desktop"]
      },
      "meta": {
        "description": "Platform should be web, mobile, tablet, or desktop"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "device_type", 
        "value_set": ["iOS", "Android", "Windows", "MacOS", "Linux"]
      },
      "meta": {
        "description": "Device type should be a valid OS"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "event_time",
        "min_value": "2024-01-01T00:00:00",
        "max_value": "2025-12-31T23:59:59"
      },
      "meta": {
        "description": "Event time should be within reasonable date range"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "event_id",
        "regex": "^EVENT-\\d{8}-[A-Z0-9]{6}$"
      },
      "meta": {
        "description": "Event ID should follow the expected format"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "customer_id",
        "regex": "^CUST-\\d{6}$"
      },
      "meta": {
        "description": "Customer ID should follow the expected format"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "session_id", 
        "regex": "^[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}$"
      },
      "meta": {
        "description": "Session ID should be a valid UUID format"
      }
    }
  ],
  "ge_cloud_id": null,
  "meta": {
    "great_expectations_version": "0.17.12",
    "expectation_suite_description": "Expectation suite for bronze layer raw user events data validation",
    "data_asset_name": "raw_user_events",
    "layer": "bronze"
  }
} 